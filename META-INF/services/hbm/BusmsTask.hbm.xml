<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                             "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sihuatech.task.entity.BusmsTask" table="tb_titan_task">
		<id name="id" type="int">
			<column name="ID" precision="0" scale="0" />
			<generator class="native" />
		</id>
		<property generated="never" lazy="false" name="name" type="string">
			<column name="name" />
		</property>
		<property generated="never" lazy="false" name="type" type="string">
			<column name="type" />
		</property>
		<property generated="never" lazy="false" name="runStatus" type="int">
			<column name="runStatus" />
		</property>
	</class>
</hibernate-mapping>